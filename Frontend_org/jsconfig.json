{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@api/*": [
                "src/api/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@common/*": [
                "src/common/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@consts/*": [
                "src/common/consts/*"
            ],
            "@locales/*": [
                "src/locales/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}